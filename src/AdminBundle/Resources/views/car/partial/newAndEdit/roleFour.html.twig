<div class="form-group col-md-12">
    <input class="btn btn-primary" type="submit" value="{% trans %}Save{% endtrans %}" />
    <a href="{{ path('admin_car_index') }}" type="button" class="btn btn-primary">{% trans %}Cancel{% endtrans %}</a>
</div>