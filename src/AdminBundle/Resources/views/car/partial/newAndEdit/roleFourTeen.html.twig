<div class="navbar-toggler car-sale-form" data-toggle="collapse"
     data-target="#carline"
     aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    Carline
</div>
<div class="collapse" id="carline">
    <div class="row">
        <div class="form-group col-md-1">
            <label for="zakupBrut">{% trans %}zakupBrut F.{% endtrans %}</label>
            {{ form_widget(form.zakupBrut) }}
        </div>
        <div class="form-group col-md-1">
            <label for="ourDiscountPrice">{% trans %}Our discount price F.{% endtrans %}</label>
            {{ form_widget(form.ourDiscountPrice) }}
        </div>
        <div class="form-group col-md-1">
            <label for="discount">{% trans %}Discount F.{% endtrans %} (%)</label>
            {{ form_widget(form.discount) }}
        </div>
        <div class="form-group col-md-1">
            <label for="vat">{% trans %}vat F.{% endtrans %}</label>
            {{ form_widget(form.vat) }}
        </div>
    </div>
    <div class="row">
        <div class="col-md-6 custom-col-md-6">
            <div class="row">
                <div class="form-group col-md-2">
                    <label for="priceRoleFive">{% trans %}Price 1 F.{% endtrans %}</label>
                    {{ form_widget(form.priceRoleFive) }}
                </div>
                <div class="form-group col-md-2">
                    <label for="priceRoleSix">{% trans %}Price 2 F.{% endtrans %}</label>
                    {{ form_widget(form.priceRoleSix) }}
                </div>
                <div class="form-group col-md-2">
                    <label for="priceRoleSeven">{% trans %}Price 3 F.{% endtrans %}</label>
                    {{ form_widget(form.priceRoleSeven) }}
                </div>
                <div class="form-group col-md-5">
                    {{ form_widget(form.showPrice) }}
                </div>
                <div class="form-group col-md-1">
                    <button id="carUploadFile" class="btn btn-primary" data-target="#modal-for-car-upload-file" data-toggle="modal" type="button">{% trans %}Upload{% endtrans %}</button>
                </div>
            </div>
            <div class="car-form-row-padding"></div>
            <div class="row">
                <div class="form-group col-md-2">
                    <label for="forwarder">{% trans %}Forwarder F.{% endtrans %}</label>
                    {{ form_widget(form.forwarder) }}
                </div>
                <div class="form-group col-md-2 date-form-input">
                    <label for="downloadDate">{% trans %}Download date F.{% endtrans %}</label>
                    {{ form_widget(form.downloadDate) }}
                </div>
                <div class="form-group col-md-2">
                    <label for="targetUnload">{% trans %}Target Unload F.{% endtrans %}</label>
                    {{ form_widget(form.targetUnload) }}
                </div>
                <div class="form-group col-md-4">
                    <label for="information">{% trans %}Information F.{% endtrans %}</label>
                    {{ form_widget(form.information, {'attr': {'class': 'car-text-area car-info-field'}}) }}
                </div>
            </div>
            <div class="row">
                <div class="form-group col-md-2">
                    <label for="shippingCost">{% trans %}Shipping cost F.{% endtrans %}</label>
                    {{ form_widget(form.shippingCost) }}
                </div>
                <div class="form-group col-md-2">
                    <label for="shippingCostType">{% trans %}PLN/EUR{% endtrans %}</label>
                    {{ form_widget(form.shippingCostType) }}
                </div>
            </div>
        </div>
        <div class="col-md-6 custom-col-md-6">
            {% if formType == 'edit' %}
                <div class="row">
                    <div class="col-md-12 custom-col-md-12-padding-left">
                        <table id="datatable-car-file" class="table image-table table-bordered table-striped table-condensed table-hover">
                            <thead>
                            <tr>
                                <th>{% trans %}File Type{% endtrans %}</th>
                                <th>{% trans %}File Name{% endtrans %}</th>
                                <th>{% trans %}User Name{% endtrans %}</th>
                                <th>{% trans %}Created At{% endtrans %}</th>
                                <th>{% trans %}Download{% endtrans %}</th>
                                <th>{% trans %}Delete{% endtrans %}</th>
                            </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            {% endif %}
        </div>
    </div>
    <div class="car-form-line"></div>
    <div class="row">
        <div class="form-group col-md-1">
            <label for="buyer">{% trans %}Buyer F.{% endtrans %}</label>
            {{ form_widget(form.user) }}
        </div>
        <div class="form-group col-md-1">
            <label for="salePriceWithOutVAT">{% trans %}Sale price without vat F.{% endtrans %}</label>
            {{ form_widget(form.salePriceWithOutVAT) }}
        </div>
        <div class="form-group col-md-1">
            <label for="salePriceWithOutVATType">{% trans %}PLN/EUR{% endtrans %}</label>
            {{ form_widget(form.salePriceWithOutVATType) }}
        </div>
        <div class="form-group col-md-1">
            <label for="salePriceWithVAT">{% trans %}Sale price with vat F.{% endtrans %}</label>
            {{ form_widget(form.salePriceWithVAT) }}
        </div>
        <div class="form-group col-md-1">
            <label for="salePriceWithVATType">{% trans %}PLN/EUR{% endtrans %}</label>
            {{ form_widget(form.salePriceWithVATType) }}
        </div>
        <div class="form-group col-md-1">
            <label for="nrPro2">{% trans %}nrPro2 F.{% endtrans %}</label>
            {{ form_widget(form.nrPro2) }}
        </div>
        <div class="form-group col-md-1 date-form-input">
            <label for="dataPro2">{% trans %}dataPro2 F.{% endtrans %}</label>
            {{ form_widget(form.dataPro2) }}
        </div>
        <div class="form-group col-md-1">
            <label for="salesInvoiceNumber">{% trans %}Sales invoice number F.{% endtrans %}</label>
            {{ form_widget(form.salesInvoiceNumber) }}
        </div>
        <div class="form-group col-md-1 date-form-input">
            <label for="dataFv2">{% trans %}dataFv2 F.{% endtrans %}</label>
            {{ form_widget(form.dataFv2) }}
        </div>
        <div class="form-group col-md-1">
            {{ form_widget(form.paidSuccess) }}
        </div>
        <div class="form-group col-md-1 date-form-input">
            <label for="invoiceDate">{% trans %}Invoice date F.{% endtrans %}</label>
            {{ form_widget(form.invoiceDate) }}
        </div>
        <div class="form-group col-md-1">
            <label for="zysk">{% trans %}zysk F.{% endtrans %}</label>
            {{ form_widget(form.zysk) }}
        </div>
    </div>
    <div class="row">
        <div class="form-group col-md-1 car-doc-button">
            {% if carId is defined %}
                {{ form_widget(form.saveCarlinePdf, {'label': 'Carline B.'}) }}
            {% endif %}
        </div>
        <div class="form-group col-md-1"></div>
        <div class="form-group col-md-1"></div>
        <div class="form-group col-md-1"></div>
        <div class="form-group col-md-1"></div>
        <div class="form-group col-md-1 date-form-input">
            <label for="carlineDate">{% trans %}carlineDate F.{% endtrans %}</label>
            {{ form_widget(form.carlineDate) }}
        </div>
        <div class="form-group col-md-1">
            <label for="carlineNumber">{% trans %}carlineNumber F.{% endtrans %}</label>
            {{ form_widget(form.carlineNumber) }}
        </div>
    </div>
</div>

